root = true

[*]
charset = utf-8
trim_trailing_whitespace = true
end_of_line = lf
insert_final_newline = true

[*.clj]
indent_style = space
indent_size = 2

[*.cljs]
indent_style = space
indent_size = 2

[*.cljc]
indent_style = space
indent_size = 2